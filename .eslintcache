seState(true);\n\n  useEffect(() => {\n    let urlParams = new URLSearchParams(window.location.search).get(\"url\");\n    if (urlParams != null && urlParams.length) {\n      setVideoUrl(urlParams);\n    } else {\n      setVideoUrl(DEFAULT_VIDEO_URL);\n    }\n  }, []);\n\n  const mimicApiCallLoading = setTimeout(() => {\n    setLoader(false);\n  }, 3000);\n\n  useEffect(() => {\n    return () => clearTimeout(mimicApiCallLoading);\n  }, []);\n\n  return (\n    <Layout>\n      <main className={styles.videoContainer}>\n        <Navbar src={Logo}/>\n        {loader ? (\n          <ContentLoader />\n        ) : (\n          <section className={styles.videoWrapper}>\n            <div className={styles.videoInner}>\n              <VideoPlayer\n                src={videoUrl}\n                videoPlaying={videoPlaying}\n                setVideoPlaying={setVideoPlaying}\n              />\n              \n              <AddBookmark\n                setVideoPlaying={setVideoPlaying}\n                savedBookmarks={savedBookmarks}\n                setBookmark={setBookmark}\n              />\n              <ChangeVideo\n                setVideoUrl={setVideoUrl}\n                setLoader={setLoader}\n                setBookmark={setBookmark}\n              />\n            </div>\n            <div className={styles.stampsInner}>\n              <Stamps savedBookmarks={savedBookmarks} />\n            </div>\n          </section>\n        )}\n      </main>\n    </Layout>\n  );\n};\n\nexport default Index;\n","/home/tanisk/Video-Bookmarking-App/src/pages/NotFound/index.js",[],"/home/tanisk/Video-Bookmarking-App/src/pages/Bookmark/ChangeVideo.js",["100"],"import React, { useEffect, useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport Button from \"../../components/Buttons\";\nimport FormInput from \"../../components/FormInput\";\n\nconst styles = {\n  formWrapper: \"px-6 pb-6 flex min-w-full\",\n  formInput: `border-gray-400 transition-all duration-200 